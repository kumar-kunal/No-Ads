{
    "manifest_version": 2,
    "name": "No Ads",
    "version":"1.0",
    "content_scripts": [{
        "matches":    ["<all_urls>"],
        "js":         ["content.js"],
        "run_at":     "document_end",
        "all_frames": false
    }],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action":{
        "default_icon": "./icon.png"
    }

}